#include<algorithm>
#include<cstring>
void printSubstrings(char str[]){
  /* Don't write main().
   *  Don't read input, it is passed as function argument.
   * Print output as specified in the question.
   */
int l=strlen(str);
 for (int i=0; i <l; i++)
    {
        
        for (int j=i; j<l; j++)
        {
           
            for (int k=i; k<=j; k++)
                cout << str[k];
 
            cout << endl;
        }
    }

}
