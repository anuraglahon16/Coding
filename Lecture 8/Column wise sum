#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
	int a[10000][10000];
	
  int m,n;
  int sum=0;
  cin>>m>>n;
  for(int i=0;i<m;i++)
  {
      for(int j=0;j<n;j++)
      {
          cin>>a[i][j];
      }
  }

for(int j=0;j<n;j++)
{   sum=0;
    for(int i=0;i<m;i++)
    {
        sum+=a[i][j];
    }
    cout<<sum<<" ";
    
}
cout<<endl;
}


