void swapAlternate(int arr[],int size){
 
  /* Don't write main().
     Don't read input, it is passed as function argument.
     Taking input and printing output is handled automatically.
  */
  int temp,i;
  if(size%2==0){
  for(int i=0;i<size;i+=2)
  {
      temp=arr[i];
      arr[i]=arr[i+1];
      arr[i+1]=temp;
  }
  }
  else{
      for(int i=0;i<size-1;i+=2)
      {
          temp=arr[i];
          arr[i]=arr[i+1];
          arr[i+1]=temp;
      }
      
  }


}
