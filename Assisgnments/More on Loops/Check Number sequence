#include<iostream>
using namespace std;
int main()
{
long a[150],b,cnt1=0,cnt2=0,i,j,k,x,n,y;
cin>>n;
for(i=0;i<n;i++){
    cin>>a[i];
}
for(j=0;j<n;j++){
    for(k=j+1;k<=j+1;k++){
        if(a[j]>a[k]){
            cnt1++;
        }
    }
}
x=cnt1;
for(j=x;j<n;j++){
    for(k=j+1;k<=j+1;k++){
        if(a[j]<=a[k]){
            cnt2++;
        }
    }
}
y=cnt2;
if(x+y==n){
    cout<<"true";
}else{
cout<<"false";
}

}
