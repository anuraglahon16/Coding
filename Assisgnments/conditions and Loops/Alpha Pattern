#include<iostream>
using namespace std;


int main(){
    int n;
    cin>>n;
    char c=65;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
        {
            cout<<c;
        }
        c=c+1;
        cout<<endl;
    }
    
    /*  Read input as specified in the question.
     * Print output as specified in the question.
     */
    
}
