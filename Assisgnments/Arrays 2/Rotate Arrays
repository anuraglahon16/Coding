void Rotate(int arr[], int d, int size) {
    /* Don't write main().
     * Don't read input, it is passed as function argument.
     * No need to print or return the output.
     * Taking input and printing the output is handled automatically.
     */

int i;
for(i=0;i<d;i++)
{
    int temp,i;
    temp=arr[0];
    for(i=0;i<size-1;i++)
    {
        arr[i]=arr[i+1];
    }
    arr[i]=temp;
}
}
